\begin{tikzpicture}[scale=1]
\begin{axis}[
	axis lines=middle,
	ticks=none,
	xlabel={$x$}, xlabel style=right,
	ylabel={$y$}, ylabel style=above,
	xmin=-5.0,xmax=10,
	ymin=-3,ymax=5]
